{
  "name": "mentorship-platform",
  "private": true,
  "version": "1.0.0",
  "author": "Talha Shafeeq",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "cross-env NODE_OPTIONS=\"--loader ts-node/esm --experimental-specifier-resolution=node\" node src/server/index.ts",
    "dev:server:modern": "tsx src/server/index.ts",
    "dev:legacy": "concurrently \"npm run dev:client\" \"npm run dev:server:legacy\"",
    "dev:server:legacy": "cross-env NODE_OPTIONS=--experimental-specifier-resolution=node node --loader ts-node/esm src/server/index.ts",
    "build": "vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "node dist/server/index.js",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview:vite": "vite preview",
    "install:deps": "npm install --legacy-peer-deps",
    "postinstall": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\""
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@stripe/react-stripe-js": "^2.6.0",
    "@stripe/stripe-js": "^2.2.0",
    "@types/react-helmet": "^6.1.11",
    "@types/uuid": "^10.0.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dexie": "^4.0.11",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "firebase": "^11.6.0",
    "framer-motion": "^12.6.3",
    "googleapis": "^148.0.0",
    "lru-cache": "^7.14.0",
    "lucide-react": "^0.483.0",
    "pouchdb-adapter-idb": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.21.1",
    "rxdb": "^16.11.0",
    "rxjs": "^7.8.2",
    "stripe": "^18.0.0",
    "uuid": "^11.1.0",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.26.10",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.2.2",
    "vite": "^6.2.2"
  }
}
